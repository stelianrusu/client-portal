{
  "routes": [
    {
      "route": "*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/login",
      "rewrite": "/.auth/login/aad"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["registeredusers"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}
